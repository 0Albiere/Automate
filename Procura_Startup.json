{
  "objective": "Encontrar startups que atendam parcialmente aos critérios de PMF, escala, monetização, retenção, buzz orgânico e viabilidade de clonagem local",
  "time_reference": "2025",
  "matching_rules": {
    "min_criteria_to_qualify": 8,
    "ideal_criteria_to_qualify": 10,
    "scoring_method": "weighted_match_percentage",
    "disqualify_if": [
      "regulatory_risk = high",
      "gross_margin_post_infra < 40"
    ]
  },
  "search_scope": [
    "news",
    "venture_capital",
    "social_media",
    "traffic_analytics",
    "app_stores",
    "developer_tools",
    "payment_stack",
    "reddit",
    "linkedin"
  ],
  "criteria_pool": {
    "valuation_min_usd": 500000000,
    "recent_round_min_usd": 100000000,
    "user_growth_yoy_min_percent": 200,
    "estimated_mau_min": 1000000,
    "d30_retention_inferred_min_percent": 20,
    "paying_intent_last_30_days_min": 50,
    "linkedin_new_hires_last_6_months_min": 50,
    "pricing_min_usd_per_month": 20,
    "stripe_required": true,
    "reviews_min_rating": 4.5,
    "reviews_min_count": 1000,
    "organic_positive_mentions_x_per_month_min": 100,
    "big_tech_partnership_required": false,
    "direct_copycats_with_funding_over_50m_max": 5,
    "reddit_pain_mentions_last_90_days_min": 200,
    "tam_min_usd": 10000000000,
    "tam_growth_yoy_min_percent": 20,
    "founder_feels_pain": true,
    "founder_daily_usage_intent": true,
    "improvement_factor_min": 3,
    "mvp_max_build_time_weeks": 4,
    "open_source_apis_allowed": true,
    "access_to_1000_users_channel": ["X", "Reddit BR"],
    "price_discount_vs_original_min_percent": 50,
    "perceived_value_multiplier_min": 2,
    "cac_payback_max_months": 3,
    "ltv_min_months": 12,
    "competitor_copy_speed_days_min": 60,
    "model_cost_stability_required": true,
    "local_integrations_required": ["Pix", "MercadoPago"],
    "retention_estimated_similar_min_percent": 30,
    "organic_channel_required": ["TikTok BR", "X devs"],
    "gross_margin_post_infra_min_percent": 60,
    "regulatory_risk": "low"
  },
  "output_requirements": {
    "return_top_n": 10,
    "for_each_project_return": [
      "name",
      "description",
      "matched_criteria_count",
      "matched_criteria_list",
      "missing_criteria_list",
      "final_match_score_percent",
      "estimated_valuation",
      "last_round_amount",
      "estimated_mau",
      "pricing",
      "payment_stack",
      "organic_buzz_metrics",
      "linkedin_hiring_signal",
      "reddit_pain_signal",
      "tam_estimate",
      "clone_viability_score",
      "unit_economics_estimate",
      "regulatory_risk",
      "evidence_links"
    ]
  }
}
